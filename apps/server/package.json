{
  "name": "server",
  "private": true,
  "version": "1.0.0",
  "description": "Server for Hello.",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run && vitest run --config test/vitest.e2e-config.ts",
    "test:cov": "vitest run --coverage --config test/vitest.e2e-config.ts",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "seeder:run": "mikro-orm seeder:run"
  },
  "engines": {
    "pnpm": ">=10.0.0",
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@keyv/redis": "^5.1.5",
    "@mikro-orm/cli": "^6.6.4",
    "@mikro-orm/core": "^6.6.4",
    "@mikro-orm/migrations": "^6.6.4",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.6.4",
    "@mikro-orm/seeder": "^6.6.4",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@types/ms": "^2.1.0",
    "axios": "^1.13.2",
    "cache-manager": "^7.2.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "i": "^0.3.7",
    "ioredis": "^5.9.2",
    "libphonenumber-js": "^1.12.34",
    "ms": "^2.1.3",
    "nestjs-pino": "^4.5.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.15.8",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.9",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^15.15.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "unplugin-swc": "^1.5.9",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  }
}

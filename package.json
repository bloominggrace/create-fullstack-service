{
  "name": "hello",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "pnpm": ">=10.0.0",
    "node": ">=24.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "lint": "pnpm --parallel -r lint",
    "type-check": "pnpm --parallel -r type-check",
    "build": "pnpm --parallel -r build",
    "test": "pnpm run -r --parallel test && pnpm run -r --parallel test:e2e"
  },
  "devDependencies": {
    "husky": "^9.1.7"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core"
    ]
  }
}
